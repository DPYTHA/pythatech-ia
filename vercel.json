{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admission",
      "dest": "/admission.html"
    },
    {
      "src": "/formations",
      "dest": "/formations.html"
    },
    {
      "src": "/formulaire",
      "dest": "/formulaire.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/page-confirmation",
      "dest": "/page confirmation.html"
    },
    
    {
      "src": "/(.*\\.css)",
      "dest": "/$1"
    },
   
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}